{
  "manifest_version": 2,
  "name": "Camera Extension",
  "author": "Peter Katsos ",
  "version": "1.0.1",
  "description": "Camera for Password",
  "browser_action":{
   "default_popup":"index.html"
  },
  "background":{
    "scripts":["background.js"],
    "persistent": true
  },
  "web_accessible_resources":["popup.html"],

  "permissions": ["<all_urls>", "storage", "activeTab"]
}